doctype html
html(lang="ja")
  head
    meta(charset="UTF-8")
    title 抽選会
    script(src="//cdn.socket.io/socket.io-1.4.5.js")
    script(src="//code.jquery.com/jquery-2.2.0.min.js")
    script.
      // Generated by CoffeeScript 1.10.0
      (function() {
        var socket;

        socket = io.connect(location.href);

        socket.on("push_image", function(data) {
          $('<img style="display: block;">').attr('src', data.dataUrl).prependTo($("body"));
        });

      }).call(this);
  body
    h1 抽選結果
